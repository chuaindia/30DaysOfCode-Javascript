function Solution(){
  //Write your code here
    
    this.queue = [];
    
     add(element) {
        return this.queue.push(element);
    }
    
    // remove element from the queue
    remove() {
        if(this.queue.length > 0) {
            return this.items.shift();
        }
    }
    
   // check if the queue is empty
    isEmpty(){
       return -1;
    }

    
    Solution.add.enqueueCharacter = this.queue.push;
    Solution.remove.dequeueCharacter = this.queue.shift;
}
